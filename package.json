{
  "name": "oh-my-desk",
  "version": "v1.1.2",
  "description": "desktop widget application",
  "main": "./app/main.prod.js",
  "scripts": {
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.js",
    "build:app": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "dev": "cross-env NODE_ENV=development electron -r babel-register ./app/main.js",
    "start": "lint-staged && cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.config.js --hot --inline --watch",
    "package": "yarn build:app && yarn build:main && build -c electron-builder.yml && yarn clear",
    "package:mac": "yarn build:app && yarn build:main && build -c electron-builder.yml --mac && yarn clear",
    "package:win": "yarn build:app && yarn build:main && build -c electron-builder.yml --win && yarn clear",
    "test": "jest --config jest.config.json",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage",
    "coverage": "open ./coverage/lcov-report/index.html",
    "lint": "eslint app --ext .js,.jsx",
    "precommit": "lint-staged",
    "prepush": "yarn lint",
    "clear": "rm -rf ./build && rm ./app/main.prod.js",
    "reset:mac": "rm ~/Library/Application \\Support/oh-my-desk/store.json"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahki/oh-my-desk.git"
  },
  "author": "hyunmoahn",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "electron": "1.7.1",
    "electron-builder": "^20.17.0",
    "electron-react-devtools": "^0.5.3",
    "electron-redux-devtools": "^0.3.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "flux-standard-action": "^2.0.3",
    "hoist-non-react-statics": "^2.5.5",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^3.8.2",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.4.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.21.0",
    "uuid": "^3.3.0",
    "valid-url": "^1.0.9",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8"
  }
}
