{
  "collectCoverageFrom": [
    "app/**/*.{js,jsx}"
  ],
  "moduleNameMapper": {
    "\\.(css|scss|jpg|svg)$": "<rootDir>/app/__mocks__/styleMock.js",
    "^app(.*)$": "<rootDir>/app$1",
    "^actions(.*)$": "<rootDir>/app/actions$1",
    "^assets(.*)$": "<rootDir>/app/assets$1",
    "^components(.*)$": "<rootDir>/app/components$1",
    "^constants(.*)$": "<rootDir>/app/constants$1",
    "^page(.*)$": "<rootDir>/app/page$1",
    "^process(.*)$": "<rootDir>/app/process$1",
    "^setting(.*)$": "<rootDir>/app/page/setting$1",
    "^store(.*)$": "<rootDir>/app/store$1",
    "^webview(.*)$": "<rootDir>/app/page/webview$1",
    "^utils(.*)$": "<rootDir>/app/utils$1"
  },
  "globals": {
    "__dirname": "/Users/user-name/project/oh-my-desk/app"
  },
  "setupTestFrameworkScriptFile": "<rootDir>/enzyme.config.js",
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ]
}
